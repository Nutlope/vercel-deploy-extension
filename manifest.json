{
  "manifest_version": 3,
  "name": "Vercel Deploy",
  "description": "Vercel Deploy allows you to deploy any GitHub repo to Vercel in 1 click.",
  "version": "2.0.0",
  "permissions": ["tabs"],
  "action": {
    "default_title": "Deploy to Vercel",
    "default_icon": "vercel.png"
  },
  "background": {
    "matches": ["https://github.com/*"],
    "service_worker": "background.js"
  }
}
